{
    "manifest_version": 2,
    "name": "{{{name}}}",
    "version": "{{{version}}}",
    "description": "{{{description}}}",
    "author": "{{{author}}}",
    {{#unless disable.chrome.content_scripts}}
    {{#if content_scripts}}
    "content_scripts": {{json content_scripts}},
    {{/if}}
    {{/unless}}
    {{#unless disable.chrome.homepage_url}}
    {{#if homepage_url}}
    "homepage_url": "{{{homepage_url}}}",
    {{/if}}
    {{/unless}}
    {{#unless disable.chrome.background}}
    {{#if background}}
    "background": {{json background}},
    {{/if}}
    {{/unless}}
    {{#unless disable.chrome.content_security_policy}}
    {{#if content_security_policy}}
    "content_security_policy": "{{{content_security_policy}}}",
    {{/if}}
    {{/unless}}
    {{#unless disable.chrome.permissions}}
    {{#if permissions}}
    "permissions": {{json permissions}},
    {{/if}}
    {{/unless}}
    {{#unless disable.chrome.web_accessible_resources}}
    {{#if web_accessible_resources}}
    "web_accessible_resources": {{json web_accessible_resources}},
    {{/if}}
    {{/unless}}
    {{#unless disable.chrome.browser_action}}
    {{#if browser_action}}
    "browser_action": {{json browser_action}},
    {{/if}}
    {{/unless}}
    {{#unless disable.chrome.chrome_url_overrides}}
    {{#if chrome_url_overrides}}
    "chrome_url_overrides": {{json chrome_url_overrides}},
    {{/if}}
    {{/unless}}
    {{#unless disable.chrome.chrome_settings_overrides}}
    {{#if chrome_settings_overrides}}{{#if homepage_url}}
    "chrome_settings_overrides": {{json chrome_settings_overrides}},
    {{/if}}{{/if}}
    {{/unless}}
    {{#unless disable.chrome.omnibox}}
    {{#if omnibox.keyword}}
    "omnibox": {
        "keyword": "{{omnibox.keyword}}"
    },
    {{/if}}
    {{/unless}}
    "icons": {
        "16": "icon16.png",
        "64": "icon64.png",
        "128": "icon128.png",
        "256": "icon256.png"
    }
}
